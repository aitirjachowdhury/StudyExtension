{
    "name": "RUHacks Education",
    "description": "Track your study time and Award yourself for being productive!",
    "version": "1.0",
    "manifest_version": 2,
      "browser_action": {
        "default_icon": "RU_icon.png"
      },
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": [
        "tabs"],
    "content_scripts": [
      {
        "matches": ["https://www.facebook.com/"],
        "js": ["content.js"]
      }
    ]

}
